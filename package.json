{
  "name": "linkedin-analytics-spark",
  "version": "1.0.0",
  "description": "LinkedIn Analytics & Growth Advisor - Production-Ready Spark Application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "preview": "vite preview",
    "preview:host": "vite preview --host",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist .vite-cache",
    "lint": "echo 'Linting not configured - add ESLint if needed'",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "health-check": "node -e \"console.log('Health check passed')\"",
    "start": "npm run preview"
  },
  "dependencies": {
    "@phosphor-icons/react": "^2.0.15",
