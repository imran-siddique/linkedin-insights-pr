    "@vitest/ui": "^3.2.4",
    "axios": "^1.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^10.16.16",
    "jsdom": "^26.1.0",
    "lucide-react": "^0.537.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "sonner": "^1.3.1",
