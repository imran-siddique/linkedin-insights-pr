# LinkedIn Analytics & Growth Advisor Environment Configuration
# Copy this file to .env.local and configure your settings

# Application Environment
NODE_ENV=production
VITE_APP_NAME=LinkedIn Analytics & Growth Advisor
VITE_APP_VERSION=1.0.0

# API Configuration (if needed in future)
# VITE_API_BASE_URL=https://api.example.com
# VITE_API_TIMEOUT=10000

# Feature Flags
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_REPORTING=true

# Rate Limiting
VITE_MAX_REQUESTS_PER_MINUTE=60
VITE_SCRAPING_RATE_LIMIT=5

# Cache Configuration
VITE_CACHE_TTL=300000
VITE_MAX_CACHE_SIZE=100