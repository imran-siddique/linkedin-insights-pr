# Environment Variables
NODE_ENV=development
PORT=3001

# API Configuration
API_KEYS=development-key,production-key-1,production-key-2

# Frontend Configuration
FRONTEND_URL=http://localhost:3000

# OpenAI API (if using real AI services)
OPENAI_API_KEY=your_openai_api_key_here

# LinkedIn API (if integrating with LinkedIn)
LINKEDIN_CLIENT_ID=your_linkedin_client_id
LINKEDIN_CLIENT_SECRET=your_linkedin_client_secret
LINKEDIN_REDIRECT_URI=http://localhost:3001/auth/linkedin/callback

# Database Configuration (for future use)
# DATABASE_URL=postgresql://username:password@localhost:5432/linkedin_analytics

# Redis Configuration (for advanced caching)
# REDIS_URL=redis://localhost:6379

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=debug